{
  "name": "node-notifier",
  "version": "7.0.1",
  "description": "A Node.js module for sending notifications on native Mac, Windows (post and pre 8) and Linux (or Growl as fallback)",
  "keywords": [
    "notification center",
    "mac os x 10.8",
    "notify",
    "terminal-notifier",
    "notify-send",
    "growl",
    "windows 8 notification",
    "toaster",
    "notification"
  ],
  "homepage": "https://github.com/mikaelbr/node-notifier#readme",
  "bugs": {
    "url": "https://github.com/mikaelbr/node-notifier/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikaelbr/node-notifier.git"
  },
  "license": "MIT",
  "author": "Mikael Brevik",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "example": "node ./example/message.js",
    "example:mac": "node ./example/advanced.js",
    "example:mac:input": "node ./example/macInput.js",
    "example:windows": "node ./example/toaster.js",
    "example:windows:actions": "node ./example/toaster-with-actions.js",
    "lint": "eslint example/*.js lib/*.js notifiers/*.js test/**/*.js index.js",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/_test-matchers.js"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/[^_]*.js"
  },
  "dependencies": {
    "growly": "^1.3.0",
    "is-wsl": "^2.1.1",
    "semver": "^7.2.1",
    "shellwords": "^0.1.1",
    "uuid": "^7.0.3",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4"
  },
  "_from": "node-notifier@7.0.1",
  "_integrity": "sha512-VkzhierE7DBmQEElhTGJIoiZa1oqRijOtgOlsXg32KrJRXsPy0NXFBqWGW/wTswnJlDCs5viRYaqWguqzsKcmg==",
  "_resolved": "https://registry.npmjs.org/node-notifier/-/node-notifier-7.0.1.tgz"
}